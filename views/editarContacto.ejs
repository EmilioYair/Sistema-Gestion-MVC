<%- include("templates/header.ejs", {titulo:"Editar Contacto"}) %>
    <form action="/editarContacto" id="formulario" methos="POST">
        <input type="text" name="id" value="<%= contactoBD._id %>">
        <label>Nombre</label>
        <input class="form-control mb-3" type="text" name="nombre" id="nombre" placeholder="Nombre" value="<%= contactoBD.nombre %>">

        <label>Edad</label>
        <input class="form-control mb-3" type="text" name="edad" id="edad" placeholder="Edad" value="<%= contactoBD.edad %>">

        <button type="submit" class="btn btn-primary">Editar Contacto</button>
    </form>
<%- include("templates/footer.ejs") %>
